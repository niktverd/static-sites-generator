{
    "name": "generate-static-sites",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start -p $PORT",
        "prepare": "husky install",
        "check-types": "tsc --pretty --noEmit",
        "check-format": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
        "check-lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
        "stylelint": "stylelint \"**/*.css\" --fix",
        "test-all": "npm run stylelint && npm run check-format && npm run check-lint && npm run check-types",
        "test-build": "npm run stylelint && npm run check-format && npm run check-lint && npm run check-types && npm run build",
        "export": "next build && next export",
        "c": "node ./customize.js"
    },
    "dependencies": {
        "@types/cookie": "^0.5.1",
        "@types/jsonwebtoken": "^8.5.8",
        "cookie": "^0.5.0",
        "firebase": "^9.9.3",
        "jose": "^4.9.0",
        "jsonwebtoken": "^8.5.1",
        "next": "^12.3.2",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@types/node": "18.0.6",
        "@types/react": "18.0.15",
        "@types/react-dom": "18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.30.7",
        "@typescript-eslint/parser": "^5.30.7",
        "@yandex-cloud/eslint-config": "^1.1.0",
        "@yandex-cloud/prettier-config": "^1.0.0",
        "eslint": "^8.20.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-next": "12.2.2",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-css": "^0.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.30.1",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "prettier": "^2.7.1",
        "stylelint": "^14.9.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-standard": "^26.0.0",
        "stylelint-order": "^5.0.0",
        "stylelint-order-config-standard": "^0.1.3",
        "typescript": "4.7.4"
    },
    "lint-staged": {
        "*.{js,ts}": "eslint --fix"
    }
}
